PREFIX prov: <http://www.w3.org/ns/prov-o/>
PREFIX void: <http://rdfs.org/ns/void#>

SELECT distinct ?dataset (count(*) as ?count)
WHERE {
  GRAPH ?g  {
    ?ephemeral a void:Dataset; prov:specializationOf ?dataset .
  }
}group by ?dataset

